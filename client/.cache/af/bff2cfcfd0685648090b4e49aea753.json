ories.vue","name":null,"original":{"line":158,"column":0},"generated":{"line":1,"column":1666}},{"source":"components/Categories.vue","name":null,"original":{"line":158,"column":0},"generated":{"line":1,"column":1668}},{"source":"components/Categories.vue","name":null,"original":{"line":158,"column":0},"generated":{"line":1,"column":1673}},{"source":"components/Categories.vue","name":null,"original":{"line":158,"column":0},"generated":{"line":1,"column":1678}},{"source":"components/Categories.vue","name":null,"original":{"line":159,"column":0},"generated":{"line":1,"column":1679}},{"source":"components/Categories.vue","name":null,"original":{"line":159,"column":0},"generated":{"line":1,"column":1684}},{"source":"components/Categories.vue","name":null,"original":{"line":160,"column":0},"generated":{"line":1,"column":1692}},{"source":"components/Categories.vue","name":null,"original":{"line":160,"column":0},"generated":{"line":1,"column":1698}},{"source":"components/Categories.vue","name":null,"original":{"line":161,"column":0},"generated":{"line":1,"column":1708}},{"source":"components/Categories.vue","name":null,"original":{"line":161,"column":0},"generated":{"line":1,"column":1713}},{"source":"components/Categories.vue","name":null,"original":{"line":161,"column":0},"generated":{"line":1,"column":1715}},{"source":"components/Categories.vue","name":null,"original":{"line":161,"column":0},"generated":{"line":1,"column":1724}},{"source":"components/Categories.vue","name":null,"original":{"line":161,"column":0},"generated":{"line":1,"column":1729}},{"source":"components/Categories.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":1,"column":1744}},{"source":"components/Categories.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":1,"column":1752}},{"source":"components/Categories.vue","name":null,"original":{"line":52,"column":0},"generated":{"line":1,"column":1760}}],"sources":{"components/Categories.vue":"<template>\n  <div>\n      <div class=\"container\">\n        <div class=\"categories d-flex\" >\n          <div class=\"category border rounded\" v-for=\"(category,i) in categories\" :key=\"i\">  \n            <div class=\"board text-dark bg-white  p-2\">\n              <div class=\"card-title text-center\">\n                <div class=\"deletecategory\" @click=\"deletecategory(category.id)\">\n                </div>\n                <button class=\"add btn btn-primary\" @click=\"showmodal(category.id)\">+Task</button>     \n                <h3>{{category.name}}</h3>\n              </div>\n              <div class=\"card-body\">     \n                <draggable  :list=\"category.Tasks\" :options='{group: {name:\"categories\"}}'  @end=\"drop\">      \n                  <transition-group tag=\"div\" v-bind:id=\"category.id\"> \n                    <div class=\"item\" v-for=\"task in category.Tasks\" :key=\"task.id\" v-bind:id=\"task.id\">\n                      <Card :islogin=\"islogin\" :task=\"task\"></Card>\n                    </div>\n                  </transition-group>\n                </draggable>\n              </div>\n            </div>\n          </div>\n          <div class=\"category border rounded\">\n              <div class=\"board text-dark bg-white  p-2\">\n                <div class=\"card-title text-center\">\n                  <h3>New Category</h3>\n                </div>\n                <div class=\"card-body\">     \n                  <form @submit.prevent=\"addcategory\">\n                      <div class=\"form-group\">\n                        <input type=\"text\" class=\"form-control\" placeholder=\"Category\" v-model=\"formcategory\">\n                        <small class=\"form-text text-muted\">Don't too long...</small>\n                      </div>\n                      <div class=\"text-right\">\n                        <button type=\"submit\" class=\"btn btn-primary\">Submit</button>\n                      </div>\n                  </form>\n                </div>\n              </div>\n            </div>\n        </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport draggable from 'vuedraggable'\nimport instance from '../apis/axios'\nimport {Swal,Toast} from '../apis/sweetalert'\nimport Card from \"./Card\"\nexport default {\n  components: {\n    draggable,\n    Card\n  },\n  props:['islogin','modal'],\n  data(){\n    return {\n      categories:[],\n      formcategory:''\n    }\n  },\n  mounted(){\n    this.load()\n  },\n  sockets: {\n    connect() {\n      console.log('socket connected')\n    },\n    task(val) {\n      this.load()\n    }\n  },\n  methods:{\n    showmodal(id){\n      this.modal.CategoryId = id\n      this.modal.showModal = true\n    },\n    load(){\n      instance\n      .get('task',{\n        headers: {'token': this.islogin.token}\n      })\n      .then(response => {\n        console.log(response.data)\n        this.categories = response.data\n      })\n      .catch(err => {\n        console.log(err.response)\n      })\n    },\n    \n    \n    drop(evt){\n      var taskId = evt.item.id\n      var categoryId = evt.to.id\n      instance  \n        ({\n            method:\"patch\",\n            url:`task/${taskId}`,\n            data:{\n              category : categoryId\n            },\n            headers: {'token': this.islogin.token}\n        })\n        .then(response => {\n          console.log(response)\n        })\n        .catch(err => {\n          Swal.fire({\n            icon: 'error',\n            title: 'Oops...',\n            text: err.response.data.message,\n          })\n        })\n    },\n    addcategory(){\n      instance\n      ({\n        method:\"POST\",\n        url:\"category\",\n        headers: {'token': this.islogin.token},\n        data:{\n          categoryname: this.formcategory\n        }\n      })\n      .then(response => {\n        Swal.fire(\n          'Success Add Category',\n          'success'\n        )\n        this.formcategory = null\n      })\n      .catch(err => {\n        console.log(err.response)\n        Swal.fire({\n          icon: 'error',\n          title: 'Oops...',\n          text: JSON.stringify(err.response.data),\n        })\n      })\n    },\n    deletecategory(id){\n      instance\n      .delete(`category/${id}`,{\n        headers: {'token': this.islogin.token}\n      })\n      .then(response => {\n        console.log(response.data)\n        Swal.fire(\n          'Success Delete Category',\n          'success'\n        )\n      })\n      .catch(err => {\n        console.log(err.response)\n        Swal.fire({\n          icon: 'error',\n          title: 'Oops...',\n          text: err.response.data.message,\n        })\n      })\n    },\n  },\n  \n}\n</script>\n<style>\n\n</style>"},"lineCount":null}},"error":null,"hash":"5eb0f7af3fecc895fe7d65c1be16a54f","cacheData":{"env":{}}}